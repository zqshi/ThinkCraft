# ThinkCraft 后端环境配置示例
# 复制此文件为 .env 并填写实际配置

# 服务器配置
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:8080

# DeepSeek API 配置
# 从 https://platform.deepseek.com 获取API密钥
# 注册账号 -> 充值余额 -> 创建API Key -> 复制到下方
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# PostgreSQL数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=thinkcraft
DB_USER=postgres
DB_PASSWORD=your_password_here

# 日志级别
LOG_LEVEL=debug
