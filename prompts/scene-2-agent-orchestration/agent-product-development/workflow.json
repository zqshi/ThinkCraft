{
  "workflow_id": "agent-product",
  "workflow_name": "Agent产品开发流程",
  "description": "用于开发ReAct Agent、Function Calling Agent等AI Agent产品",
  "phases": [
    {
      "phase_id": "requirement",
      "phase_name": "需求设计阶段",
      "agents": [
        {
          "agent_id": "product-demand-manager",
          "role": "主导",
          "tasks": ["需求澄清", "需求设计", "挑战回应"]
        },
        {
          "agent_id": "product-research-analyst",
          "role": "支持",
          "tasks": ["市场调研", "竞品分析"]
        },
        {
          "agent_id": "product-demand-challenge",
          "role": "质量保证",
          "tasks": ["需求设计挑战"]
        },
        {
          "agent_id": "product-demand-refine",
          "role": "文档精炼",
          "tasks": ["生成LLM版需求文档"]
        }
      ],
      "dependencies": [],
      "outputs": ["需求设计文档-LLM版"]
    },
    {
      "phase_id": "strategy",
      "phase_name": "战略设计阶段",
      "agents": [
        {
          "agent_id": "strategy-design",
          "role": "主导",
          "tasks": ["战略设计", "挑战回应"]
        },
        {
          "agent_id": "strategy-design-challenge",
          "role": "质量保证",
          "tasks": ["战略设计挑战"]
        }
      ],
      "dependencies": ["requirement"],
      "outputs": ["战略设计文档"]
    },
    {
      "phase_id": "development",
      "phase_name": "开发阶段",
      "agents": [
        {
          "agent_id": "agentscope-react-developer",
          "role": "主导",
          "tasks": ["Agent开发"]
        },
        {
          "agent_id": "test-expert",
          "role": "测试",
          "tasks": ["Prompt测试", "工具测试"]
        },
        {
          "agent_id": "devops",
          "role": "部署",
          "tasks": ["部署配置", "运维"]
        },
        {
          "agent_id": "performance",
          "role": "优化",
          "tasks": ["Prompt优化", "Token优化"]
        }
      ],
      "dependencies": ["strategy"],
      "outputs": ["Agent产品代码", "测试报告", "部署文档"]
    }
  ]
}
